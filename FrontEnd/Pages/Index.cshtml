@page
@model IndexModel
@{
        ViewData["Title"] = "Home page";

    <div class="agenda">
        <h1>My Conference @System.DateTime.Now.Year</h1>

        @foreach (var timeSlot in Model.Session)
        {
            <h4>@timeSlot.Key?.ToString("HH:mm")</h4>
            <ul>
                @foreach (var session in timeSlot)
                {
                    <li>@session.SessionTitle</li>
                }
            </ul>
        }
    </div>
}